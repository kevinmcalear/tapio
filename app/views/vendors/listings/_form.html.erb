<div class="new-listing-form">
  <%= form_for [@vendor, @listing] do |f| %>

  <%= f.label :name %><br/>
  <%= f.text_field :name %>
  <br/>

  <%= f.label :listing_pic_url %><br/>
  <%= f.text_field :listing_pic_url %>
  <br/>

  <%= f.label :description %><br/>
  <%= f.text_area :description %>
  <br/>

  <%= f.label :address %><br/>
  <%= f.text_field :address %>
  <br/>

  <%= f.label :city %><br/>
  <%= f.text_field :city %>
  <br/>

  <%= f.label :state %><br/>
  <%= f.text_field :state %>
  <br/>

  <%= f.label :zip %><br/>
  <%= f.text_field :zip %>
  <br/>

  <%= f.label :rate %><br/>
  <%= f.text_field :rate %>
  <br/>

  <%= f.label :quantity %><br/>
  <%= f.text_field :quantity %>
  <br/>

  <br/>
  <br/>
  <%= f.submit @listing.id.nil? ? "Add Listing" : "Update Listing" %>
  <% end %>
</div>
